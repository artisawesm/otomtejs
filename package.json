{
  "name": "otomtejs",
  "version": "0.1.4",
  "description": "The toolkit that automates code optimization and framework setup for your front end projects on the web.",
  "main": "index.js",
  "scripts": {
    "oto:setup": "npm install",
    "oto": "cross-env NODE_ENV=development webpack-dev-server",
    "oto:dev": "npm run oto:development",
    "oto:development": "cross-env NODE_ENV=development webpack --hide-modules",
    "oto:prod": "npm run oto:production",
    "oto:production": "cross-env NODE_ENV=production webpack --hide-modules",
    "oto:vue": "npm uninstall react react-dom react-router-dom --save-dev && npm install vue vue-template-compiler --save-dev && cross-env NODE_ENV=vue node ./config/presets.js",
    "oto:react": "npm uninstall vue vue-template-compiler --save-dev && npm install react react-dom react-router-dom --save-dev && cross-env NODE_ENV=react node ./config/presets.js",
    "oto:wp-setup": "cross-env NODE_ENV=wp node ./config/presets.js",
    "oto:wp-dev": "cross-env NODE_ENV=development webpack --watch --hide-modules",
    "oto:wp-prod": "cross-env NODE_ENV=production webpack --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artisawesm/otomtejs.git"
  },
  "keywords": [
    "Automation",
    "React",
    "Vue",
    "JavaScript",
    "PWA",
    "SPA",
    "SEO",
    "Boilerplate",
    "SCSS",
    "Webpack4"
  ],
  "author": "Art San Diego <asd.artsandiego@gmail.com> (https://artisawesm.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artisawesm/otomtejs/issues"
  },
  "homepage": "https://github.com/artisawesm/otomtejs#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "bulma": "^0.7.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "extract-loader": "^3.1.0",
    "fs-extra": "^7.0.1",
    "group-css-media-queries-loader": "^3.0.2",
    "hot-module-replacement": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "popper.js": "^1.15.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue-loader": "^15.7.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  }
}
